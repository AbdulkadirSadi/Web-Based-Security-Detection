# API Keys
VIRUSTOTAL_API_KEY=your_virustotal_api_key_here

# Database Connection
DATABASE_CONNECTION_STRING=Server=your_server;Database=your_db;User Id=your_user;Password=your_password

# Backend API Configuration
BACKEND_API_URL=http://localhost:7260/api

# Security Settings
ENABLE_PROCESS_MONITORING=true
AUTO_TERMINATE_MALICIOUS_PROCESSES=true
AUTO_DELETE_MALICIOUS_FILES=false
VIRUSTOTAL_DETECTION_THRESHOLD=1

# Monitoring Configuration
INCLUDE_SUBDIRECTORIES=true
ENABLE_ALERTS=true 